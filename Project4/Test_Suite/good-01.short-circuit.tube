# More complex boolean logic that uses short-circuiting.

int a = 1;
print a%7+2 == 3 && a*10/7 == 1 || a=0;
print a%7+2 == 3 && a*10/7 == 1 || a=0;
print a%7+2 == 3 && a*10/7 == 1 && a=0;
print a%7+2 == 3 && a*10/7 == 1 || a=0;
a += 1;
print a%7+2 == 3 && a*10/7 == 1 || a=0;

# Should output 1 1 0 0 1  (one per line).

